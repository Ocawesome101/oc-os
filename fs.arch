local function itemize( t )\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    local item = next( t )\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    if item == nil then\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        return \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"nothing\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    end\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    local text = \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    while item do\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        text = text .. item\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        local nextItem = next( t, item )\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        if nextItem ~= nil then\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
            local nextNextItem = next( t, nextItem )\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
            if nextNextItem == nil then\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
                text = text .. \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\" and \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
            else\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
                text = text .. \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\", \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
            end\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        end\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
        item = nextItem\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    end\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
    return text\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
end\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
