local function doMenuItem( _n )\\\\\\\\\\\\\\\
    tMenuFuncs[tMenuItems[_n]]()\\\\\\\\\\\\\\\
    if bMenu then\\\\\\\\\\\\\\\
        bMenu = false\\\\\\\\\\\\\\\
        term.setCursorBlink( true )\\\\\\\\\\\\\\\
    end\\\\\\\\\\\\\\\
    redrawMenu()\\\\\\\\\\\\\\\
end\\\\\\\\\\\\\\\
